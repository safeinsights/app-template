{
    "extends": "next/core-web-vitals",
    "rules": {
        "no-console": ["error", { "allow": ["warn", "error"] }],
        "no-unused-vars": [
            "error",
            { "ignoreRestSiblings": true, "varsIgnorePattern": "_+", "argsIgnorePattern": "^_" }
        ],
        "semi": ["error", "never"]
    }
}
